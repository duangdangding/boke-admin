<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.lsh.bokeadmin.mapper.CategorysMapper">
    <insert id="addCate" parameterType="com.lsh.bokeadmin.entry.Categorys">
        insert into categorys (cate_name,description) values(#{cateName,jdbcType=VARCHAR},#{description,jdbcType=VARCHAR})
    </insert>
</mapper>